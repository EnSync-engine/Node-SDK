  "author": "EnSync Team",
  "license": "ISC",
  "files": [
    "grpc.js",
    "message-builder.js",
    "index.d.ts",
    "ensync.proto",
    "README.md",
    "CHANGELOG.md",
    "API.md"
  ],
  "dependencies": {
    "@grpc/grpc-js": "^1.14.0",
    "@grpc/proto-loader": "^0.7.15",
    "ensync-utils": "^0.1.0"
  },
  "devDependencies": {
    "prettier": "^3.2.5"
  }
}
