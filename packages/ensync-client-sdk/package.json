{
  "name": "ensync-client-sdk",
  "version": "0.2.2",
  "description": "gRPC client for EnSync Engine - High-performance message-driven communication",
  "main": "grpc.js",
  "types": "index.d.ts",
  "scripts": {
    "format": "prettier --write \"**/*.{js,jsx,ts,tsx,json,md}\"",
    "format:check": "prettier --check \"**/*.{js,jsx,ts,tsx,json,md}\"",
    "prepublishOnly": "npm run format:check"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/EnSync-engine/Node-SDK.git"
  },
  "keywords": [
    "ensync",
    "grpc",
    "message-driven",
    "messaging",
    "real-time",
    "pub-sub",
    "event-driven"
